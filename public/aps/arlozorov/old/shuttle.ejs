			<!-- Info -->
		<div class="info-container">
			<label class="checkbox-inline">
				<input type="checkbox" ng-model="sh_show" /> Shuttle sensors
			</label>
			<label class="checkbox-inline">
				<input type="checkbox" ng-model="silomat_show" /> Silomat sensors
			</label>
			<div class="pull-right">
				<span ng-bind="shuttleStatus(sh1_card[4].bytes[0].bits[7].status)" ng-class="shuttleClass(sh1_card[4].bytes[0].bits[7].status)"></span>
			</div>
			<p>
				<!--<h4 class="text-left">Shuttle travelling</h4>-->
				<div class="panel panel-default">
					<table class="table table-bordered table-condensed" >
						<tr>
							<td class="text-left col-1">Horizontal Encoder</td>
							<td class="text-center col-2"><span class="glyphicon glyphicon-flash"></span></td>
							<td class="text-center col-3"><strong class="text-primary" ng-bind="sh1.lasers[0].position"></strong> x 2mm</td>
							<td class="text-center col-4"><strong class="text-primary" ng-bind="sh1.lasers[0].destination"></strong> x 2mm</td>
						</tr>
						<tr>
							<td class="text-left col-1">Shuttle travelling motors</td>
							<td class="text-center col-2" ng-bind-html="motorStatus(sh1.motors[0].status)" ng-class="motorStatusClass(sh1.motors[0].status)"></td>
							<td class="text-center col-3"><strong class="text-primary" ng-bind="sh1.motors[0].speed"></strong> Hz</td>
							<td class="text-center col-4"><strong class="text-primary" ng-bind="sh1.motors[0].current"></strong> A</td>
						</tr>
						<!--
						<tr>
							<td class="text-left col-1">Cable-reel drum motor</td>
							<td class="text-center col-2" ng-bind-html="motorStatus(sh1.motors[1].status)" ng-class="motorStatusClass(sh1.motors[1].status)"></td>
							<td class="text-center col-3"><strong class="text-primary" ng-bind="sh1.motors[1].speed"></strong> Hz</td>
							<td class="text-center col-4"><strong class="text-primary" ng-bind="sh1.motors[1].current"></strong> A</td>
						</tr>
						-->
					</table>
				</div>
